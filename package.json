{
  "name": "@neoskop/flow-bump",
  "version": "1.1.0-rc.0",
  "description": "A merge of git-flow and version bumping",
  "main": "bundle/flow-bump.bundle.js",
  "module": "dist/public_api.js",
  "jsnext:main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "scripts": {
    "test:raw": "TS_NODE_CACHE=false mocha --require ts-node/register 'src/**.spec.ts'",
    "test:watch": "npm run test:raw -- --watch --watch-extension ts",
    "test:coverage": "rimraf coverage && nyc -e '.ts' -r html -r text npm run test:raw",
    "test": "npm run test:coverage && nyc report -r text-lcov | coveralls",
    "clean": "rimraf lib dist index.d.ts index.js index.js.map",
    "prebuild": "npm run test:coverage",
    "build": "npm run clean && tsc",
    "publish-next": "npm run build && npm publish --tag next",
    "publish-latest-only": "npm run build && npm publish",
    "publish-latest": "npm run publish-latest-only && npm dist-tag add @neoskop/flow-bump@`jq '.version' package.json -r` next"
  },
  "bin": {
    "flow-bump": "./bin/cli.js",
    "fb": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoskop/flow-bump.git"
  },
  "keywords": [
    "npm",
    "version",
    "git",
    "flow",
    "bump"
  ],
  "author": "Mark Wecke <mark.wecke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoskop/flow-bump/issues"
  },
  "homepage": "https://github.com/neoskop/flow-bump#readme",
  "devDependencies": {
    "@types/chai": "4.2.0",
    "@types/colors": "1.2.1",
    "@types/execa": "2.0.0",
    "@types/fs-extra": "8.0.0",
    "@types/ini": "1.3.30",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.1",
    "@types/object-path": "0.11.0",
    "@types/semver": "6.0.1",
    "@types/source-map-support": "0.5.0",
    "@types/yamljs": "0.2.30",
    "@types/yargs": "13.0.2",
    "chai": "4.2.0",
    "coveralls": "3.0.6",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts",
      "dist",
      "bundle",
      "src/fixtures.ts"
    ]
  },
  "dependencies": {
    "colors": "1.3.3",
    "execa": "2.0.3",
    "fs-extra": "8.1.0",
    "ini": "1.3.5",
    "listr": "0.14.3",
    "object-path": "0.11.4",
    "rxjs": "6.5.2",
    "semver": "6.3.0",
    "source-map-support": "0.5.13",
    "split": "1.0.1",
    "stream-to-observable": "0.2.0",
    "tslib": "1.10.0",
    "yamljs": "0.3.0",
    "yargs": "13.3.0"
  }
}